mapscripts Route125_MapScripts {
	MAP_SCRIPT_ON_LOAD : Route125_OnLoad
	MAP_SCRIPT_ON_TRANSITION : Route125_OnTransition
}

script Route125_OnLoad {
	call(Route125_EventScript_OpenGarden)
	end
}

script Route125_OnTransition {
	call(Common_EventScript_DisableBPReward)
	end
}

script Route125_EventScript_OpenGarden {
	goto_if_not_defeated(TRAINER_NOLEN, Common_EventScript_NopReturn)
	goto_if_not_defeated(TRAINER_TANYA, Common_EventScript_NopReturn)
	goto_if_not_defeated(TRAINER_SHARON, Common_EventScript_NopReturn)
	goto_if_not_defeated(TRAINER_ERNEST, Common_EventScript_NopReturn)
	goto_if_not_defeated(TRAINER_KIM_AND_IRIS, Common_EventScript_NopReturn)
	goto_if_not_defeated(TRAINER_PRESLEY, Common_EventScript_NopReturn)
	goto_if_not_defeated(TRAINER_AURON, Common_EventScript_NopReturn)
	goto_if_not_defeated(TRAINER_TERRY, Common_EventScript_NopReturn)
	goto_if_not_defeated(TRAINER_HOLLY, Common_EventScript_NopReturn)
	goto_if_not_defeated(TRAINER_COLTON, Common_EventScript_NopReturn)
	setmetatile(62, 17, METATILE_General_CalmWater, FALSE)
	setmetatile(63, 17, METATILE_General_CalmWater, FALSE)
	setmetatile(62, 18, METATILE_General_CalmWater, FALSE)
	setmetatile(63, 18, METATILE_General_CalmWater, FALSE)
	return
}

script Route125_EventScript_Nolen {
	trainerbattle_single(TRAINER_NOLEN, Route125_Text_NolenIntro, Route125_Text_NolenDefeat)
	msgbox(Route125_Text_NolenPostBattle, MSGBOX_AUTOCLOSE)
	call(Route125_EventScript_TryNolenRematch)
	release
	end
}
script Route125_EventScript_TryNolenRematch {
	lock
    call(Common_EventScript_ShouldDoRematch)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_NopReturn)
	setvar(VAR_0x8003, TRAINER_NOLEN)
	setvar(VAR_0x8004, 1)
	call(Common_EventScript_CheckRecentRematchesSetBP)
	trainerbattle_no_intro(TRAINER_NOLEN, Route125_Text_NolenDefeat)
	special(UpdateRecentTrainers)
	return
}

