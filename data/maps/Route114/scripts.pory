mapscripts Route114_MapScripts {
	MAP_SCRIPT_ON_TRANSITION : Route114_OnTransition
}

script Route114_OnTransition {
	call(Common_EventScript_SetWild_0)
	call(Common_EventScript_DisableBPReward)
	end
}

script Route114_EventScript_Nurse {
	lock
	faceplayer
	msgbox(format("Want me to heal your Pokémon?"), MSGBOX_YESNO)
	call_if_eq(VAR_RESULT, YES, Common_EventScript_OutOfCenterPartyHeal)
	msgbox(format("Be careful, Trainer!"))
	release
	end
}

script Route114_EventScript_Lenny {
	trainerbattle_single(TRAINER_LENNY, Route114_Text_LennyIntro, Route114_Text_LennyDefeat)
	msgbox(Route114_Text_LennyPostBattle, MSGBOX_AUTOCLOSE)
	call(Route114_EventScript_TryLennyRematch)
	release
	end
}
script Route114_EventScript_TryLennyRematch {
	lock
    call(Common_EventScript_ShouldDoRematch)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_NopReturn)
	setvar(VAR_0x8003, TRAINER_LENNY)
	setvar(VAR_0x8004, 1)
	call(Common_EventScript_CheckRecentRematchesSetBP)
	trainerbattle_no_intro(TRAINER_LENNY, Route114_Text_LennyDefeat)
	special(UpdateRecentTrainers)
	return
}

script Route114_EventScript_Lucas {
	trainerbattle_single(TRAINER_LUCAS, Route114_Text_LucasIntro, Route114_Text_LucasDefeat)
	msgbox(Route114_Text_LucasPostBattle, MSGBOX_AUTOCLOSE)
	call(Route114_EventScript_TryLucasRematch)
	release
	end
}
script Route114_EventScript_TryLucasRematch {
	lock
    call(Common_EventScript_ShouldDoRematch)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_NopReturn)
	setvar(VAR_0x8003, TRAINER_LUCAS)
	setvar(VAR_0x8004, 1)
	if (defeated(TRAINER_ANGELINA)) {
		msgbox(format("Include Angelina and make it a double battle?"), MSGBOX_YESNO)
		if (var(VAR_RESULT) == YES) {
			trainerbattle(TRAINER_BATTLE_SET_TRAINER_B, TRAINER_ANGELINA, 0, NULL, Route114_Text_AngelinaDefeat)
			addvar(VAR_0x8004, 1)
		}
	}
	call(Common_EventScript_CheckRecentRematchesSetBP)
	trainerbattle_no_intro(TRAINER_LUCAS, Route114_Text_LucasDefeat)
	special(UpdateRecentTrainers)
	return
}

