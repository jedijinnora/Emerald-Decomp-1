mapscripts Route111_MapScripts {
	MAP_SCRIPT_ON_LOAD : Route111_OnLoad
	MAP_SCRIPT_ON_TRANSITION : Route111_OnTransition
}

script Route111_OnLoad {
	call_if_unset(FLAG_REGI_DOORS_OPENED, Route111_EventScript_CloseDesertRuins)
	call_if_unset(FLAG_DESERT_UNDERPASS_OPEN, Route111_EventScript_CloseUnderpass)
	special(SetMirageTowerVisibility)
	call_if_unset(FLAG_MIRAGE_TOWER_VISIBLE, Route111_EventScript_HideMirageTower)
	end
}

script Route111_EventScript_CloseUnderpass {
	setmetatile(32, 53, METATILE_General_RockWall_RockBase, TRUE)
	setmetatile(32, 54, METATILE_General_RockWall_SandBase, TRUE)
	return
}

script Route111_EventScript_CloseDesertRuins {
	setmetatile(29, 106, METATILE_General_RockWall_RockBase, TRUE)
	setmetatile(29, 107, METATILE_General_RockWall_SandBase, TRUE)
	return
}

script Route111_EventScript_HideMirageTower {
	setmetatile(18, 73, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(19, 73, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(20, 73, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(18, 74, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(19, 74, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(20, 74, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(18, 75, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(19, 75, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(20, 75, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(18, 76, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(19, 76, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(20, 76, METATILE_Mauville_DeepSand_Center, FALSE)
    setmetatile(18, 77, METATILE_Mauville_DeepSand_BottomMid, FALSE)
    setmetatile(19, 77, METATILE_Mauville_DeepSand_BottomMid, FALSE)
    setmetatile(20, 77, METATILE_Mauville_DeepSand_BottomMid, FALSE)
    setmetatile(18, 78, METATILE_General_SandPit_Center, FALSE)
    setmetatile(19, 78, METATILE_General_SandPit_Center, FALSE)
    setmetatile(20, 78, METATILE_General_SandPit_Center, FALSE)
	return
}

script Route111_OnTransition {
	call(Route111_EventScript_TryMoveStash)
	call(Common_EventScript_SetWild_0)
	setvar(VAR_TRAINER_HILL_IS_ACTIVE, 0)
	call(Route111_EventScript_CheckSetSandstorm)
	call(GabbyAndTy_EventScript_UpdateLocation)
	call_if_not_defeated(TRAINER_VICKY, Route111_EventScript_SetWinstratesNotDefeated)
	checkitem(ITEM_GO_GOGGLES)
	call_if_eq(VAR_RESULT, TRUE, Route111_EventScript_HideGogglesHintMan)
	if(flag(FLAG_LANDMARK_OLD_LADY_REST_SHOP)) {
		setflag(FLAG_TEMP_13)
	}
	end
}

script Route111_EventScript_SetWinstratesNotDefeated {
	clearflag(FLAG_HIDE_ROUTE_111_VICTOR_WINSTRATE)
	setflag(FLAG_HIDE_ROUTE_111_WINSTRATE_FAMILY)
	cleartrainerflag(TRAINER_VICTOR)
	cleartrainerflag(TRAINER_VICTORIA)
	cleartrainerflag(TRAINER_VIVI)
	cleartrainerflag(TRAINER_VITO)
	return
}

script Route111_EventScript_HideGogglesHintMan {
	setflag(FLAG_TEMP_1)
	return
}

script Route111_EventScript_TryMoveStash {
	goto_if_set(FLAG_HIDE_SECRET_STASH_ROUTE_111, Common_EventScript_NopReturn)
	switch(var(VAR_SECRET_STASH_STATE)) {
		case 0:
			setobjectxyperm(LOCALID_STASH, 21, 22)
		case 1:
			setobjectxyperm(LOCALID_STASH, 15, 129)
		case 2:
			setobjectxyperm(LOCALID_STASH, 7, 101)
		case 3:
			setobjectxyperm(LOCALID_STASH, 0, 25)
		default:
			setobjectxyperm(LOCALID_STASH, 27, 138)
	}
	return
}

script Route111_EventScript_Stash {
	setvar(VAR_TEMP_D, ITEM_PROTEIN)
	setvar(VAR_TEMP_E, ITEM_DAWN_STONE)
	setvar(VAR_TEMP_F, ITEM_SHINY_STONE)
	call(Common_EventScript_SecretStash)
	end
}

