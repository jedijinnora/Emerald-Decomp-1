script Route133_EventScript_Beck {
	trainerbattle_single(TRAINER_BECK, Route133_Text_BeckIntro, Route133_Text_BeckDefeat)
	msgbox(Route133_Text_BeckPostBattle, MSGBOX_AUTOCLOSE)
	call(Route133_EventScript_TryBeckRematch)
	release
	end
}
script Route133_EventScript_TryBeckRematch {
	lock
    call(Common_EventScript_ShouldDoRematch)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_NopReturn)
	setvar(VAR_0x8003, TRAINER_BECK)
	setvar(VAR_0x8004, 1)
	if (defeated(TRAINER_DIANNE)) {
		msgbox(format("Include Dianne and make it a double battle?"), MSGBOX_YESNO)
		if (var(VAR_RESULT) == YES) {
			trainerbattle(TRAINER_BATTLE_SET_TRAINER_B, TRAINER_DIANNE, 0, NULL, Route133_Text_DianneDefeat)
			addvar(VAR_0x8004, 1)
		}
	}
	call(Common_EventScript_CheckRecentRematchesSetBP)
	trainerbattle_no_intro(TRAINER_BECK, Route133_Text_BeckDefeat)
	special(UpdateRecentTrainers)
	return
}

