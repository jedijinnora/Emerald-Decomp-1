mapscripts Route133_MapScripts {
	MAP_SCRIPT_ON_TRANSITION : Route133_OnTransition
}

script Route133_OnTransition {
	call(Common_EventScript_DisableBPReward)
	end
}

script Route133_EventScript_Sign {
	setflag(FLAG_LANDMARK_SUNNY_LAGOON)
	msgbox("Sunny Lagoon", MSGBOX_SIGN)
	end
}

script Route133_EventScript_Franklin {
	trainerbattle_single(TRAINER_FRANKLIN, Route133_Text_FranklinIntro, Route133_Text_FranklinDefeat)
	msgbox(Route133_Text_FranklinPostBattle, MSGBOX_AUTOCLOSE)
	call(Route133_EventScript_TryFranklinRematch)
	release
	end
}
script Route133_EventScript_TryFranklinRematch {
	lock
    call(Common_EventScript_ShouldDoRematch)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_NopReturn)
	setvar(VAR_0x8003, TRAINER_FRANKLIN)
	setvar(VAR_0x8004, 1)
	call(Common_EventScript_CheckRecentRematchesSetBP)
	trainerbattle_no_intro(TRAINER_FRANKLIN, Route133_Text_FranklinDefeat)
	special(UpdateRecentTrainers)
	return
}

