script RustboroCity_EventScript_RivalNorth {
	lock
	setobjectxyperm(LOCALID_RIVAL, 41, 10)
	addobject(LOCALID_RIVAL)
	applymovement(LOCALID_RIVAL, RustboroCity_Movement_RivalApproachNorth)
	waitmovement(LOCALID_RIVAL)
	playse(SE_PIN)
	applymovement(LOCALID_RIVAL, Common_Movement_ExclamationMark)
	waitmovement(LOCALID_RIVAL)
	getplayerxy(VAR_TEMP_1, VAR_TEMP_2)
	switch(var(VAR_TEMP_1)) {
		case 39:
			applymovement(OBJ_EVENT_ID_PLAYER, RustboroCity_Movement_PlayerNorth_1)
		case 40:
			applymovement(OBJ_EVENT_ID_PLAYER, RustboroCity_Movement_PlayerNorth_2)
		case 41:
			applymovement(OBJ_EVENT_ID_PLAYER, RustboroCity_Movement_PlayerNorth_3)
		case 42:
			applymovement(OBJ_EVENT_ID_PLAYER, RustboroCity_Movement_PlayerNorth_4)
		case 43:
			applymovement(OBJ_EVENT_ID_PLAYER, RustboroCity_Movement_PlayerNorth_5)
		default:
	}
	waitmovement(OBJ_EVENT_ID_PLAYER)
	copyvar(VAR_0x8004, VAR_RUSTBORO_CITY_STATE)
	call(Common_EventScript_RivalBattle)
	setvar(VAR_RUSTBORO_CITY_STATE, 100)
	closemessage
	applymovement(LOCALID_RIVAL, RustboroCity_Movement_RivalDepartNorth)
	waitmovement(LOCALID_RIVAL)
	removeobject(LOCALID_RIVAL)
	call(Common_EventScript_ClearRivalAll)
	release
	end
}
