mapscripts RustboroCity_MapScripts {
	MAP_SCRIPT_ON_TRANSITION : RustboroCityOnTransition
}

script RustboroCityOnTransition {
	call_if_unset(FLAG_VISITED_RUSTBORO_CITY, RustboroCity_EventScript_VisitRustboro)
	call_if_set(FLAG_MET_ROXANNE_STEVEN, RustboroCity_EventScript_HideNolandRoxanneSteven)
	call_if_eq(VAR_RUSTBORO_CITY_STATE, 0, RustboroCity_EventScript_TrySpawnRival)
	setflag(FLAG_TEMP_1A)
	call_if_ne(VAR_RUSTBORO_CITY_STATE, 0, RustboroCity_EventScript_SetupRival)
	end
}

script RustboroCity_EventScript_VisitRustboro {
	setflag(FLAG_VISITED_RUSTBORO_CITY)
	addvar(VAR_CITIES_VISITED, 1)
	return
}

script RustboroCity_EventScript_HideNolandRoxanneSteven {
	setflag(FLAG_TEMP_15)
	setflag(FLAG_TEMP_16)
	setflag(FLAG_TEMP_17)
	setvar(VAR_TEMP_0, 1)
	return
}

script RustboroCity_EventScript_TrySpawnRival {
	goto_if_lt(VAR_CITIES_VISITED, 4, Common_EventScript_NopReturn)
	goto_if_eq(VAR_DIFFICULTY_SETTING, 2, RustboroCity_EventScript_SpawnRival)
	goto_if_lt(VAR_CITIES_VISITED, 5, Common_EventScript_NopReturn)
	RustboroCity_EventScript_SpawnRival:
	getplayerxy(VAR_TEMP_1, VAR_TEMP_2)
	if (var(VAR_TEMP_1) > 59) { 
		//east entry
		setvar(VAR_0x8004, 1)
	} elif (var(VAR_TEMP_2) > 59) { 
		//south entry
		setvar(VAR_0x8004, 2)
	} elif (var(VAR_TEMP_2) > 200) {
		//north entry
		setvar(VAR_0x8004, 0)
	} else {
		//warp entry
		setvar(VAR_0x8004, 4)
	}
	setvar(VAR_0x8005, 6)
	specialvar(VAR_RUSTBORO_CITY_STATE, MultiplyVars)
	copyvar(VAR_0x8004, VAR_RUSTBORO_CITY_STATE)
	copyvar(VAR_0x8005, VAR_BLOCKER_RIVAL_RANDOM)
	specialvar(VAR_RUSTBORO_CITY_STATE, AddVars)
	call(Common_EventScript_IncrementRivalVar)
	return
}

script RustboroCity_EventScript_SetupRival {
	//if rival has been defeated, don't do anything
	goto_if_gt(VAR_RUSTBORO_CITY_STATE, 99, Common_EventScript_NopReturn)

	//clear rival object flag
	clearflag(FLAG_TEMP_1A)

	//lock all exits if the player has been visiting new cities
	if(var(VAR_CITIES_VISITED) > 3) {
		while (var(VAR_RUSTBORO_CITY_STATE) < 25) {
			addvar(VAR_RUSTBORO_CITY_STATE, 6)
		}
	}

	//buffer city variable and call setup
	copyvar(VAR_0x8004, VAR_RUSTBORO_CITY_STATE)
	call(Common_EventScript_SetupRival)
	return
}

script RustboroCity_EventScript_Rival {
	lock
	faceplayer
	switch(var(VAR_RUSTBORO_CITY_STATE)) {
		case 1:
		case 7:
		case 13:
		case 19:
		case 25:
			call(RustboroCity_EventScript_RivalLeaf)
		case 2:
		case 8:
		case 14:
		case 20:
		case 26:
			call(RustboroCity_EventScript_RivalCalem)
		case 3:
		case 9:
		case 15:
		case 21:
		case 27:
			call(RustboroCity_EventScript_RivalDawn)
		case 4:
		case 10:
		case 16:
		case 22:
		case 28:
			call(RustboroCity_EventScript_RivalHilbert)
		case 5:
		case 11:
		case 17:
		case 23:
		case 29:
			call(RustboroCity_EventScript_RivalKris)
		case 6:
		case 12:
		case 18:
		case 24:
		case 30:
			call(RustboroCity_EventScript_RivalRei)
		default:
	}
	release
	end
}

