mapscripts LittlerootTown_ProfessorBirchsLab_MapScripts {
	
}

script LittlerootTown_ProfessorBirchsLab_EventScript_Aide {
	lock
	faceplayer
	call_if_unset(FLAG_ITEM_TM95, LittlerootTown_ProfessorBirchsLab_EventScript_Thief)
	msgbox(format("Many held items are useful in battle, but you can also earn good money by farming Wild Pokémon for Nuggets or Star Pieces."))
	release
	end
}

script LittlerootTown_ProfessorBirchsLab_EventScript_Thief {
	msgbox(format("My job? I've been collecting data on all the different items Wild Pokémon can hold."))
	msgbox(format("Here, this TM can teach your Pokémon to nab a foe's item."))
	giveitem(ITEM_TM95_THIEF)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_ShowBagIsFull)
	setflag(FLAG_ITEM_TM95)
	return
}

script LittlerootTown_ProfessorBirchsLab_EventScript_Birch {
	lock
	faceplayer
	call_if_unset(FLAG_ITEM_TM05, LittlerootTown_ProfessorBirchsLab_EventScript_GiveFalseSwipe)
	msgbox(format("Prof. Birch: Welcome back, {PLAYER}!"))
	call_if_ge(VAR_NUM_BADGES, 4, LittlerootTown_ProfessorBirchsLab_EventScript_TryGiveCatchingCharm)
	call_if_set(FLAG_SYS_GAME_CLEAR, LittlerootTown_ProfessorBirchsLab_EventScript_TryGiveOvalCharm)
	call_if_unset(FLAG_RECEIVED_SHINY_CHARM, LittlerootTown_ProfessorBirchsLab_EventScript_TryGiveShinyCharm)
	call(ProfBirch_EventScript_AskRatePokedex)
	release
	end
}

script LittlerootTown_ProfessorBirchsLab_EventScript_GiveFalseSwipe {
	msgbox(format("Welcome to my lab! I'm Professor Birch. I run the Hoenn League's Pokédex system."))
	msgbox(format("Prof. Birch: Here, all Trainers should have this TM in their pocket!"))
	giveitem(ITEM_TM05_FALSE_SWIPE)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_ShowBagIsFull)
	setflag(FLAG_ITEM_TM05)
	msgbox(format("Prof. Birch: False Swipe is a wonderful move for catching new Pokémon. No matter what, the foe will be left with at least 1 HP!"))
	release
	end
}

script LittlerootTown_ProfessorBirchsLab_EventScript_TryGiveCatchingCharm {
	checkitem(ITEM_CATCHING_CHARM)
	call_if_eq(VAR_RESULT, FALSE, LittlerootTown_ProfessorBirchsLab_EventScript_GiveCatchingCharm)
	return
}

script LittlerootTown_ProfessorBirchsLab_EventScript_GiveCatchingCharm {
	buffernumberstring(STR_VAR_1, VAR_NUM_BADGES)
	msgbox(format("Prof. Birch: Wow, you've obtained {STR_VAR_1} badges!"))
	msgbox(format("Here, I'd like you to have this."))
	giveitem(ITEM_CATCHING_CHARM)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_ShowBagIsFull)
	msgbox(format("Prof. Birch: The Catching Charm boosts your chance of a critical capture when throwing a Poké Ball."))
	msgbox(format("Prof. Birch: Keep up the good work, you're doing great!"))
	return
}

script LittlerootTown_ProfessorBirchsLab_EventScript_TryGiveOvalCharm {
	checkitem(ITEM_OVAL_CHARM)
	call_if_eq(VAR_RESULT, FALSE, LittlerootTown_ProfessorBirchsLab_EventScript_GiveOvalCharm)
	return
}

script LittlerootTown_ProfessorBirchsLab_EventScript_GiveOvalCharm {
	msgbox(format("Prof. Birch: Wow, you beat Champion Anabel! Here, I'd like you to have this."))
	giveitem(ITEM_CATCHING_CHARM)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_ShowBagIsFull)
	msgbox(format("Prof. Birch: The Oval Charm makes it more likely to find an Egg at the Pokémon Day Care."))
	msgbox(format("Prof. Birch: Keep up the fantastic work, you're doing amazing!"))
}

script LittlerootTown_ProfessorBirchsLab_EventScript_TryGiveShinyCharm {
	specialvar(VAR_RESULT, CheckPartyForShiny)
	goto_if_eq(VAR_RESULT, TRUE, LittlerootTown_ProfessorBirchsLab_EventScript_GiveShinyCharm)
	return
}

script LittlerootTown_ProfessorBirchsLab_EventScript_GiveShinyCharm {
	msgbox(format("Prof. Birch: Would you look at the colors on that Pokémon! Here, I'd like you to have this."))
	giveitem(ITEM_SHINY_CHARM)
	goto_if_eq(VAR_RESULT, FALSE, Common_EventScript_ShowBagIsFull)
	msgbox(format("Prof. Birch: The Shiny Charm makes you more likely to encounter Shiny Pokémon."))
	msgbox(format("Prof. Birch: Keep up the stellar work!"))
	return
}



